IF
	Clicked([ANYONE])
	GlobalGT("pxinshop","GLOBAL",0)
THEN
	RESPONSE #100
	ClearAllActions()
	CreateVisualEffectObject("SPDIMNDR",LastTrigger)
	Wait(1)
	FadeToColor([30.0],0)
	Wait(1)
	RestorePartyLocations()
	FadeFromColor([30.0],0)
	SetGlobal("pxinshop","GLOBAL",0)
	SetGlobal("pxpartyinshop","GLOBAL",0)
END